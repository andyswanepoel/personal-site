<section id="home" class="section section__home">
  <div class="terminal">
    <div
      class="terminal__buttons"></div>
    <div class="terminal__inner">
      <div class="terminal__text__container">
        <p class="terminal__text terminal__text__command" aria-hidden="true">
          <span>&gt;_</span>
          whoami</p>
        <h1 class="terminal__text terminal__text__response">I'm Andy&mdash;a frontend developer from Toronto!
        </h1>
      </div>
      <div class="terminal__text__container">
        <p class="terminal__text terminal__text__command" aria-hidden="true">
          <span>&gt;_</span>
          learnmore</p>
        <p class="terminal__text terminal__text__response">You can learn more about me, my skills, and my experience below.
        </p>
      </div>
    </div>
  </div>
  <picture class="img__main__container">
    <source
      media="(min-width: 1025px)"
      srcset="/img/home/andy-juggling-full.webp"
      type="image/webp">
    <source
      media="(min-width: 1025px)"
      srcset="/img/home//andy-juggling-full.png"
      type="image/png">
    <source
      media="(max-width: 1024px)"
      srcset="/img/home//andy-juggling-1024.webp"
      type="image/webp">
    <source
      media="(max-width: 1024px)"
      srcset="/img/home//andy-juggling-1024.png"
      type="image/png">
    <img
      class="img img__main"
      src="/img/home//andy-juggling-full.png"
      alt="Andy juggling Typescript, React, and Jest logos.">
  </picture>

</section>